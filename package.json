{
  "name": "react-ocean-forms-bootstrap",
  "version": "2.0.0-rc1",
  "description": "Forms components for react based on the context api.",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "private": false,
  "eslint.packageManager": "yarn",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack --watch --mode development",
    "build": "webpack -p",
    "lint": "tslint --project ."
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/enzyme.ts",
      "<rootDir>/src/utils/registerCustomMessages.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/test-utils/**"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/config/jest/tsconfig.jest.json"
      }
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "peerDependencies": {
    "moment": "^2.22.1",
    "react": "^16.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "react-datetime": "^2.16.2",
    "react-ocean-forms": "^2.0.0-rc1",
    "react-select": "^2.1.0",
    "reactstrap": "^6.1.0 || ^6.2.0 || ^6.3.0 || ^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-select": "^2.0.6",
    "@types/reactstrap": "^6.4.2",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "moment": "^2.22.2",
    "node-sass": "^4.9.4",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "23.10.4",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-consistent-codestyle": "^1.14.1",
    "tslint-eaa-contrib": "^0.1.3",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/environment-agency-austria/react-ocean-forms-bootstrap.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "forms",
    "form",
    "react-ocean-forms",
    "forms",
    "form",
    "validation",
    "react-form",
    "react-forms",
    "react-input"
  ],
  "author": "Denis Masinovic <denis.masinovic@umweltbundesamt.at>",
  "license": "MIT"
}
